#card-scan{
  width: 100vw;
  height: 30vh;
  background-color: rgb(248, 248, 248);
}
#card-scan image{
  width: 18vh;
  height: 15vh;
  margin-top: 5vh;
}
#card-scan #card-scan-text{
  color: rgb(75, 75, 75)
}
#camera{
  width: 100vw;
  height: 30vh;
  position: relative;
}
#camera cover-image{
  position: absolute;
  top: 1em;
  left: 1em;
  width: 2em;
  height: 2em;
}